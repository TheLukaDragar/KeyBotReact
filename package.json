{
  "name": "keybotreact",
  "version": "1.0.0",
  "main": "index",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android -d",
    "ios": "npx expo run:ios",
    "test": "jest --verbose --testPathPattern 'full_test'",
    "test_internal": "jest --verbose --coverage --runInBand",
    "build": "npx eas-cli build --profile preview --platform android",
    "build:local": "npx eas-cli build --profile preview --platform android --local"
  },
